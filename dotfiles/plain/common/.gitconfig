	name = Jimmy Briggs
	email = jimmy.briggs@jimbrig.com
	signingKey = 4435EBBFAA4519665AB21E6DC2C187416B18BA59
[gpg]
    program = /mnt/c/Program Files/Git/usr/bin/gpg.exe
[commit]
    gpgSign = true
[default]
    protocol = ssh
[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
    branch = magenta
[help]
    autocorrect = 1
[apply]
    whitespace = fix
[tag]
    forceSignAnnotated = true
[submodule]
    recurse = true